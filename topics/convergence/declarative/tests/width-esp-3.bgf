<?xml version="1.0" encoding="UTF-8"?>
<bgf:grammar xmlns:bgf="http://planet-sl.org/bgf">
	<bgf:production>
		<nonterminal>x</nonterminal>
		<bgf:expression>
			<choice>
				<bgf:expression>
					<terminal>bar</terminal>
				</bgf:expression>
				<bgf:expression>
					<plus>
						<bgf:expression>
							<nonterminal>foo</nonterminal>
						</bgf:expression>
					</plus>
				</bgf:expression>
			</choice>
		</bgf:expression>
	</bgf:production>
</bgf:grammar>

<?xml version="1.0" encoding="UTF-8"?>
<mbgf:sequence xmlns:bgf="http://planet-sl.org/bgf" xmlns:mbgf="http://planet-sl.org/mbgf">
	<sources>
		<src name="a">selectables-multi-1.bgf</src>
		<src name="b">selectables-multi-2.bgf</src>
		<src name="c">selectables-multi-3.bgf</src>
	</sources>
	<selectables>
		<src name="b">
			<bgf:production>
				<nonterminal>x</nonterminal>
				<bgf:expression>
					<sequence>

							<marked>
								<bgf:expression>
									<selectable>
										<selector>FOO</selector>
										<bgf:expression>
											<nonterminal>foo</nonterminal>
										</bgf:expression>
									</selectable>
								</bgf:expression>
							</marked>
						</bgf:expression>
						<bgf:expression>
							<marked>
								<bgf:expression>
									<selectable>
										<selector>BAR</selector>
										<bgf:expression>
											<nonterminal>bar</nonterminal>
										</bgf:expression>
									</selectable>
								</bgf:expression>
							</marked>
						</bgf:expression>
					</sequence>
				</bgf:expression>
			</bgf:production>
		</src>
	</selectables>
</mbgf:sequence>

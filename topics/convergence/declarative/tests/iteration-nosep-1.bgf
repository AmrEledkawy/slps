<?xml version="1.0" encoding="UTF-8"?>
<bgf:grammar xmlns:bgf="http://planet-sl.org/bgf">
	<bgf:production>
		<nonterminal>x</nonterminal>
		<bgf:expression>
			<plus>
				<bgf:expression>
					<nonterminal>x</nonterminal>
				</bgf:expression>
			</plus>
		</bgf:expression>
	</bgf:production>
</bgf:grammar>

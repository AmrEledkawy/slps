	<selectables>
		<src name="ant">
			<bgf:production>
				<nonterminal>expr</nonterminal>
				<bgf:expression>
					<sequence>
						<bgf:expression>
							<terminal>(</terminal>
						</bgf:expression>
						<bgf:expression>
							<marked>
								<bgf:expression>
									<selectable>
										<selector>e</selector>
										<bgf:expression>
											<nonterminal>expr</nonterminal>
										</bgf:expression>
									</selectable>
								</bgf:expression>

	<selectables>
		<src name="ant">
			<bgf:production>
				<nonterminal>expr</nonterminal>
				<bgf:expression>
					<sequence>
						<bgf:expression>
							<terminal>if</terminal>
						</bgf:expression>
						<bgf:expression>
							<marked>
								<bgf:expression>
									<selectable>
										<selector>c</selector>
										<bgf:expression>
											<nonterminal>expr</nonterminal>

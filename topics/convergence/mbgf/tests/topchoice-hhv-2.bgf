<?xml version="1.0" encoding="UTF-8"?>
<bgf:grammar xmlns:bgf="http://planet-sl.org/bgf">
	<bgf:production>
		<nonterminal>x</nonterminal>
		<bgf:expression>
			<choice>
				<bgf:expression>
					<nonterminal>x</nonterminal>
				</bgf:expression>
				<bgf:expression>
					<star>
						<bgf:expression>
							<sequence>
								<bgf:expression>
									<nonterminal>op</nonterminal>
								</bgf:expression>
								<bgf:expression>
									<nonterminal>x</nonterminal>
								</bgf:expression>
							</sequence>
						</bgf:expression>
					</star>
				</bgf:expression>
				<bgf:expression>
					<nonterminal>y</nonterminal>
				</bgf:expression>
			</choice>
		</bgf:expression>
	</bgf:production>
</bgf:grammar>

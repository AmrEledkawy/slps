<?xml version="1.0" encoding="UTF-8"?>
<bgf:grammar xmlns:bgf="http://planet-sl.org/bgf">
	<bgf:production>
		<nonterminal>x</nonterminal>
		<bgf:expression>
			<sequence>
				<bgf:expression>
					<selectable>
						<selector>f</selector>
						<bgf:expression>
							<nonterminal>foo</nonterminal>
						</bgf:expression>
					</selectable>
				</bgf:expression>
				<bgf:expression>
					<nonterminal>bar</nonterminal>
				</bgf:expression>
			</sequence>
		</bgf:expression>
	</bgf:production>
</bgf:grammar>

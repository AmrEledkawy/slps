				<bgf:expression>
					<nonterminal>string</nonterminal>
				</bgf:expression>
				<bgf:expression>
					<nonterminal>tag</nonterminal>
				</bgf:expression>
				<bgf:expression>
					<nonterminal>string</nonterminal>
				</bgf:expression>

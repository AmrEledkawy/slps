<?xml version="1.0" encoding="utf-8"?>
<xldf:evolutionSequence
  xmlns:ldf="http://planet-sl.org/ldf"
  xmlns:xldf="http://planet-sl.org/xldf"
  xmlns:bgf="http://planet-sl.org/bgf"
  xmlns:xbgf="http://planet-sl.org/xbgf">

  <xldf:retitle>
    <from>
      <title>document</title>
    </from>
    <to>Document</to>
  </xldf:retitle>
  
  <xldf:retitle>
    <from>
      <title>titlePage</title>
    </from>
    <to>Title page</to>
  </xldf:retitle>
  <xldf:combine>
    <section>status</section>
    <with>titlePage</with>
  </xldf:combine>

  <xldf:retitle>
    <from>
      <title>body</title>
    </from>
    <to>Standardisation bodies</to>
  </xldf:retitle>

  <xldf:retitle>
    <from>
      <title>generated</title>
    </from>
    <to>Placeholders for generated content</to>
  </xldf:retitle>

  <xldf:retitle>
    <from>
      <title>simpleSection</title>
    </from>
    <to>Simple sections</to>
  </xldf:retitle>

  <xldf:retitle>
    <from>
      <title>simpleText</title>
    </from>
    <to>Simple text</to>
  </xldf:retitle>
  <xldf:combine>
    <section>simpleTextElement</section>
    <with>simpleText</with>
  </xldf:combine>

  <xldf:retitle>
    <from>
      <title>simpleTable</title>
    </from>
    <to>Simple tables</to>
  </xldf:retitle>
  <xldf:combine>
    <section>tableRow</section>

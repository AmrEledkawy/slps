  </xldf:combine>

  <xldf:retitle>
    <from>
      <title>topSection</title>
    </from>
    <to>Front and back matter sections</to>
  </xldf:retitle>

  <xldf:retitle>
    <from>
      <title>simpleFigure</title>
    </from>
    <to>Simple figures</to>
  </xldf:retitle>

  <xldf:combine>
    <section>figureType</section>
    <with>simpleFigure</with>
  </xldf:combine>

  <xldf:retitle>
    <from>
      <title>frontList</title>
    </from>
    <to>Front matter lists</to>
  </xldf:retitle>
  <xldf:combine>
    <section>listOfTerms</section>
    <with>frontList</with>
  </xldf:combine>

  <xldf:retitle>
    <from>
      <title>simpleList</title>
    </from>
    <to>Simple lists</to>
  </xldf:retitle>

  <xldf:retitle>
    <from>
      <title>lexicalSection</title>
    </from>
    <to>Lexical part</to>
  </xldf:retitle>

  <xldf:retitle>
    <from>
      <title>structuredSection</title>
    </from>
    <to>Main sections</to>
  </xldf:retitle>
  <!--<xldf:combine>
    <section>complexType-listOfInternalLinks</section>
    <with>group-structuredSectionElement</with>
  </xldf:combine>
  
  <xldf:transformGrammar>
    <target>group-structuredSectionElement</target>
    <xbgf:inline>listOfInternalLinks</xbgf:inline>
  </xldf:transformGrammar>-->
  <xldf:combine>
    <section>structuredSectionElement</section>
    <with>structuredSection</with>
  </xldf:combine>
  <xldf:drop>
    <section>math</section>
  </xldf:drop>

  <xldf:retitle>
    <from>
      <title>normative</title>
    </from>
    <to>Normative sections</to>
  </xldf:retitle>

  <xldf:retitle>
    <from>
      <title>informative</title>
    </from>
    <to>Informative sections</to>
  </xldf:retitle>

  <xldf:drop>
    <section>runnableType</section>
  </xldf:drop>

  <xldf:retitle>
    <from>
      <title>keyword</title>
    </from>
    <to>Keywords, links and plain text</to>
  </xldf:retitle>
  <xldf:combine>
    <section>code</section>
    <with>keyword</with>
  </xldf:combine>
  <xldf:combine>
    <section>link</section>
    <with>keyword</with>
  </xldf:combine>
  <xldf:combine>
    <section>mixedType</section>
    <with>keyword</with>
  </xldf:combine>


</xldf:evolutionSequence>
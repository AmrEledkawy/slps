<?xml version="1.0" encoding="UTF-8"?>
<bgf:grammar xmlns:bgf="http://planet-sl.org/bgf">
	<bgf:production>
		<nonterminal>foo</nonterminal>
		<bgf:expression>
			<choice>
				<bgf:expression>
					<nonterminal>b</nonterminal>
				</bgf:expression>
				<bgf:expression>
					<nonterminal>a</nonterminal>
				</bgf:expression>
				<bgf:expression>
					<nonterminal>r</nonterminal>
				</bgf:expression>
			</choice>
		</bgf:expression>
	</bgf:production>
</bgf:grammar>

<xbgf:sequence xmlns:xbgf="http://planet-sl.org/xbgf">
  <xbgf:downcase/>
  <xbgf:undefine>visitor</xbgf:undefine>
  <xbgf:rename>
    <selector>
      <from>args</from>
      <to>arg</to>
    </selector>
  </xbgf:rename>
  <xbgf:rename>
    <selector>
      <from>functions</from>
      <to>function</to>
    </selector>
  </xbgf:rename>
  <xbgf:reroot>
    <root>program</root>
  </xbgf:reroot>
</xbgf:sequence>

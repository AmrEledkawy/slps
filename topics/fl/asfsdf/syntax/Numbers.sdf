module syntax/Numbers

imports basic/Integers
imports basic/Whitespace

exports
  sorts 
	FL-INT

context-free syntax
	"-"? NatCon	-> FL-INT
	"add-int" "(" FL-INT "," FL-INT ")" -> FL-INT

hiddens
	context-free start-symbols FL-INT

	context-free syntax
		"toInt" "(" Integer ")" -> FL-INT

	variables
		"N" [0-9]* -> NatCon
		"I" [0-9]* -> FL-INT
		"H" [0-9]* -> Integer

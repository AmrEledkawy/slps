module syntax/Numbers

imports basic/Integers
imports basic/Whitespace

exports
  sorts 
	Int

context-free syntax
	"-"? NatCon	-> Int
	"add-int" "(" Int "," Int ")" -> Int

hiddens
	context-free start-symbols Int

	context-free syntax
		"toInt" "(" Integer ")" -> Int

	variables
		"N" [0-9]* -> NatCon
		"I" [0-9]* -> Int
		"H" [0-9]* -> Integer

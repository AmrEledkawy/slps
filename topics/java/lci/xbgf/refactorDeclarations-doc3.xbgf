    </in>
  </xbgf:unfold>

  <!-- ISSUE REFACTOR FOLDING -->
  <xbgf:extract>
    <bgf:production>
      <nonterminal>VariableDeclaratorRest</nonterminal>
      <bgf:expression>
        <sequence>
          <bgf:expression>
            <nonterminal>BracketsOpt</nonterminal>
          </bgf:expression>
          <bgf:expression>

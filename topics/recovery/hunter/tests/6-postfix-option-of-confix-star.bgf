<?xml version="1.0" encoding="UTF-8"?>
<bgf:grammar xmlns:bgf="http://planet-sl.org/bgf">
	<bgf:production>
		<nonterminal>zzz</nonterminal>
		<bgf:expression>
			<nonterminal>aaa</nonterminal>
		</bgf:expression>
	</bgf:production>
	<bgf:production>
		<nonterminal>yyy</nonterminal>
		<bgf:expression>
			<optional>
				<bgf:expression>
					<star>
						<bgf:expression>
							<nonterminal>aaa</nonterminal>
						</bgf:expression>
					</star>
				</bgf:expression>
			</optional>
		</bgf:expression>
	</bgf:production>
	<bgf:production>
		<nonterminal>xxx</nonterminal>
		<bgf:expression>
			<optional>
				<bgf:expression>
					<star>
						<bgf:expression>
							<sequence>
								<bgf:expression>
									<nonterminal>aaa</nonterminal>
								</bgf:expression>
								<bgf:expression>
									<nonterminal>bbb</nonterminal>
								</bgf:expression>
							</sequence>
						</bgf:expression>
					</star>
				</bgf:expression>
			</optional>
		</bgf:expression>
	</bgf:production>
	<bgf:production>
		<nonterminal>www</nonterminal>
		<bgf:expression>
			<sequence>
				<bgf:expression>
					<nonterminal>aaa</nonterminal>
				</bgf:expression>
				<bgf:expression>
					<optional>
						<bgf:expression>
							<star>
								<bgf:expression>
									<sequence>
										<bgf:expression>
											<nonterminal>bbb</nonterminal>
										</bgf:expression>
										<bgf:expression>
											<nonterminal>ccc</nonterminal>
										</bgf:expression>
									</sequence>
								</bgf:expression>
							</star>
						</bgf:expression>
					</optional>
				</bgf:expression>
				<bgf:expression>
					<nonterminal>ddd</nonterminal>
				</bgf:expression>
			</sequence>
		</bgf:expression>
	</bgf:production>
</bgf:grammar>

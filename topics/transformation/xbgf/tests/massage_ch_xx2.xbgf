<?xml version="1.0" encoding="utf-8"?>
<xbgf:sequence
  xmlns:bgf="http://planet-sl.org/bgf"
  xmlns:xbgf="http://planet-sl.org/xbgf">
  <xbgf:massage>
    <bgf:expression>
      <star>
        <bgf:expression>
          <nonterminal>bar</nonterminal>
        </bgf:expression>
      </star>
    </bgf:expression>
    <bgf:expression>
      <choice>
        <bgf:expression>
          <selectable>
            <selector>one</selector>
            <bgf:expression>
              <star>
                <bgf:expression>
                  <nonterminal>bar</nonterminal>
                </bgf:expression>
              </star>
            </bgf:expression>
          </selectable>
        </bgf:expression>
        <bgf:expression>
          <selectable>
            <selector>two</selector>
            <bgf:expression>
              <star>
                <bgf:expression>
                  <nonterminal>bar</nonterminal>
                </bgf:expression>
              </star>
            </bgf:expression>
          </selectable>
        </bgf:expression>
      </choice>
    </bgf:expression>
  </xbgf:massage>
  <xbgf:vertical>
    <nonterminal>foo</nonterminal>
  </xbgf:vertical>
  <xbgf:massage>
    <bgf:expression>
      <star>
        <bgf:expression>
          <nonterminal>bar</nonterminal>
        </bgf:expression>
      </star>
    </bgf:expression>
    <bgf:expression>
      <choice>
        <bgf:expression>
          <epsilon/>
        </bgf:expression>
        <bgf:expression>
          <plus>
            <bgf:expression>
              <nonterminal>bar</nonterminal>
            </bgf:expression>
          </plus>
        </bgf:expression>
      </choice>
    </bgf:expression>
    <in>
      <label>one</label>
    </in>
  </xbgf:massage>
  <xbgf:massage>
    <bgf:expression>
      <star>
        <bgf:expression>
          <nonterminal>bar</nonterminal>
        </bgf:expression>
      </star>
    </bgf:expression>
    <bgf:expression>
      <choice>
        <bgf:expression>
          <optional>
            <bgf:expression>
              <nonterminal>bar</nonterminal>
            </bgf:expression>
          </optional>
        </bgf:expression>
        <bgf:expression>
          <plus>
            <bgf:expression>
              <nonterminal>bar</nonterminal>
            </bgf:expression>
          </plus>
        </bgf:expression>
      </choice>
    </bgf:expression>
    <in>
      <label>two</label>
    </in>
  </xbgf:massage>
  <xbgf:extract>
    <bgf:production>
      <nonterminal>barplus</nonterminal>
      <bgf:expression>
        <plus>
          <bgf:expression>
            <nonterminal>bar</nonterminal>
          </bgf:expression>
        </plus>
      </bgf:expression>
    </bgf:production>
    <in>
      <label>one</label>
    </in>
  </xbgf:extract>
  <xbgf:unlabel>
    <label>one</label>
  </xbgf:unlabel>
  <xbgf:unlabel>
    <label>two</label>
  </xbgf:unlabel>
  <xbgf:horizontal>foo</xbgf:horizontal>
  <xbgf:distribute>
    <nonterminal>foo</nonterminal>
  </xbgf:distribute>
</xbgf:sequence>
<?xml version="1.0" encoding="utf-8"?>
<xbgf:sequence
  xmlns:bgf="http://planet-sl.org/bgf"
  xmlns:xbgf="http://planet-sl.org/xbgf">
  <xbgf:add><horizontal>
    <bgf:production>
      <nonterminal>N</nonterminal>
      <bgf:expression>
        <choice>
          <bgf:expression>
            <marked>
              <bgf:expression>
                <terminal>x</terminal>
              </bgf:expression>
            </marked>
          </bgf:expression>
          <bgf:expression>
            <nonterminal>a</nonterminal>
          </bgf:expression>
          <bgf:expression>
            <nonterminal>b</nonterminal>
          </bgf:expression>
        </choice>
      </bgf:expression>
    </bgf:production></horizontal>
  </xbgf:add>
</xbgf:sequence>

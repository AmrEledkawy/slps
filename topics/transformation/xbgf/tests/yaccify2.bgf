<?xml version="1.0" encoding="UTF-8"?>

<bgf:grammar xmlns:bgf="http://planet-sl.org/bgf">
  <bgf:production>
    <nonterminal>B</nonterminal>
    <bgf:expression>
      <sequence>
        <bgf:expression>
          <nonterminal>A</nonterminal>
        </bgf:expression>
        <bgf:expression>
          <star>
            <bgf:expression>
              <sequence>
                <bgf:expression>
                  <terminal>t</terminal>
                </bgf:expression>
                <bgf:expression>
                  <nonterminal>A</nonterminal>
                </bgf:expression>
              </sequence>
            </bgf:expression>
          </star>
        </bgf:expression>
      </sequence>
    </bgf:expression>
  </bgf:production>
</bgf:grammar>
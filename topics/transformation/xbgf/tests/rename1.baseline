<?xml version="1.0" encoding="UTF-8"?>

<xns1:grammar xmlns:xns1="http://planet-sl.org/bgf">
  <xns1:production>
    <label>constant</label>
    <nonterminal>expr</nonterminal>
    <xns1:expression>
      <nonterminal>int</nonterminal>
    </xns1:expression>
  </xns1:production>
  <xns1:production>
    <label>binary_expr</label>
    <nonterminal>expr</nonterminal>
    <xns1:expression>
      <sequence>
	<xns1:expression>
	  <nonterminal>expr</nonterminal>
	</xns1:expression>
	<xns1:expression>
	  <selectable>
	    <selector>op</selector>
	    <xns1:expression>
	      <nonterminal>binary_op</nonterminal>
	    </xns1:expression>
	  </selectable>
	</xns1:expression>
	<xns1:expression>
	  <nonterminal>expr</nonterminal>
	</xns1:expression>
      </sequence>
    </xns1:expression>
  </xns1:production>
  <xns1:production>
    <label>unary</label>
    <nonterminal>expr</nonterminal>
    <xns1:expression>
      <sequence>
	<xns1:expression>
	  <selectable>
	    <selector>op</selector>
	    <xns1:expression>
	      <nonterminal>unary_op</nonterminal>
	    </xns1:expression>
	  </selectable>
	</xns1:expression>
	<xns1:expression>
	  <nonterminal>expr</nonterminal>
	</xns1:expression>
      </sequence>
    </xns1:expression>
  </xns1:production>
</xns1:grammar>
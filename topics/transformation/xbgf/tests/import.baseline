<?xml version="1.0" encoding="UTF-8"?>
<bgf:grammar xmlns:bgf="http://planet-sl.org/bgf">
  <bgf:production>
    <nonterminal>X</nonterminal>
    <bgf:expression>
      <sequence>
        <bgf:expression>
          <terminal>a</terminal>
        </bgf:expression>
        <bgf:expression>
          <terminal>b</terminal>
        </bgf:expression>
      </sequence>
    </bgf:expression>
  </bgf:production>
  <bgf:production>
    <nonterminal>A</nonterminal>
    <bgf:expression>
      <sequence>
        <bgf:expression>
          <nonterminal>B</nonterminal>
        </bgf:expression>
        <bgf:expression>
          <nonterminal>X</nonterminal>
        </bgf:expression>
      </sequence>
    </bgf:expression>
  </bgf:production>
  <bgf:production>
    <nonterminal>B</nonterminal>
    <bgf:expression>
      <choice>
        <bgf:expression>
          <nonterminal>A</nonterminal>
        </bgf:expression>
        <bgf:expression>
          <epsilon/>
        </bgf:expression>
      </choice>
    </bgf:expression>
  </bgf:production>
</bgf:grammar>
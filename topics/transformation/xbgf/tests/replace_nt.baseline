<?xml version="1.0" encoding="UTF-8"?>

<xns1:grammar xmlns:xns1="http://planet-sl.org/bgf">
  <xns1:production>
    <label>l1</label>
    <nonterminal>a</nonterminal>
    <xns1:expression>
      <nonterminal>x</nonterminal>
    </xns1:expression>
  </xns1:production>
  <xns1:production>
    <label>l2</label>
    <nonterminal>b</nonterminal>
    <xns1:expression>
      <nonterminal>y</nonterminal>
    </xns1:expression>
  </xns1:production>
  <xns1:production>
    <label>l3</label>
    <nonterminal>b</nonterminal>
    <xns1:expression>
      <star>
	<xns1:expression>
	  <nonterminal>y</nonterminal>
	</xns1:expression>
      </star>
    </xns1:expression>
  </xns1:production>
  <xns1:production>
    <label>l4</label>
    <nonterminal>c</nonterminal>
    <xns1:expression>
      <nonterminal>x</nonterminal>
    </xns1:expression>
  </xns1:production>
</xns1:grammar>
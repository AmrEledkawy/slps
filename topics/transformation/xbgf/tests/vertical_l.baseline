<?xml version="1.0" encoding="UTF-8"?>
<bgf:grammar xmlns:bgf="http://planet-sl.org/bgf">
	<bgf:production>
		<label>nodecs</label>
		<nonterminal>decs</nonterminal>
		<bgf:expression>
			<epsilon/>
		</bgf:expression>
	</bgf:production>
	<bgf:production>
		<nonterminal>decs</nonterminal>
		<bgf:expression>
			<nonterminal>dec</nonterminal>
		</bgf:expression>
	</bgf:production>
	<bgf:production>
		<nonterminal>decs</nonterminal>
		<bgf:expression>
			<sequence>
				<bgf:expression>
					<nonterminal>dec</nonterminal>
				</bgf:expression>
				<bgf:expression>
					<nonterminal>decs</nonterminal>
				</bgf:expression>
			</sequence>
		</bgf:expression>
	</bgf:production>
</bgf:grammar>

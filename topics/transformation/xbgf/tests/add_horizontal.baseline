<?xml version="1.0" encoding="UTF-8"?>
<bgf:grammar xmlns:bgf="http://planet-sl.org/bgf">
  <bgf:production>
    <nonterminal>N</nonterminal>
    <bgf:expression>
      <choice>
        <bgf:expression>
          <terminal>x</terminal>
        </bgf:expression>
        <bgf:expression>
          <nonterminal>a</nonterminal>
        </bgf:expression>
        <bgf:expression>
          <nonterminal>b</nonterminal>
        </bgf:expression>
      </choice>
    </bgf:expression>
  </bgf:production>
</bgf:grammar>